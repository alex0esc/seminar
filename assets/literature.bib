@techreport{seemann23,
	abstract = {Die rasante Entwicklung von Systemen K\"{u}nstlicher Intelligenz wie Chat-GPT, die inhaltlich und sprachlich \"{u}berzeugende Texte generieren k\"{o}nnen, hat eine intensive Debatte ausgel\"{o}st. Es stellt sich die Frage, welche Auswirkungen solche Systeme auf die Prozesse und Arbeitsweisen zum Beispiel in Wissens- und Kreativberufen haben werden. Diese Literaturstudie wertet den aktuellen Stand der Debatte aus. Sie f\"{u}hrt in die technische Grundlage, die so genannten "Large Language Models", ein und untersucht abschlie\ss{}end, welche Auswirkungen auf die Arbeitswelt zu erwarten sind.},
	address = {D\"{u}sseldorf},
	author = {Michael Seemann},
	copyright = {https://www.econstor.eu/dspace/Nutzungsbedingungen; https://creativecommons.org/licenses/by/4.0/de/legalcode},
	keywords = {330; KI; ChatGPT; AI; Wissensarbeit; Kreativberufe},
	language = {ger},
	number = {304},
	publisher = {Hans-B\"{o}ckler-Stiftung},
	title = {K\"{u}nstliche Intelligenz, Large Language Models, ChatGPT und die Arbeitswelt der Zukunft},
	type = {Working Paper Forschungsf\"{o}rderung},
	url = {https://hdl.handle.net/10419/278731},
	year = {2023}
}


